<!DOCTYPE html>
<html>
  <head>
    <title>DunderMifflinPetShop</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <% if flash[:errors]%>
    <% flash[:errors].each do |error| %>
        <h4><%= error %></h4>
        <% end%>
    <% end%>
    <%= yield %>
  </body>
</html>
